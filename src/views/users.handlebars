<div class="container">
  <div style="padding: 80px" ; class="row">


<p style="text-align: center; font-size:50px; font-weight: bold";>ALL REGISTERED USERS:</p>

    {{#each users}}
    <div class="card" style="width: 18rem;">
      <p style="text-align: center;" class="card-text"> Id: {{this._id}}</p>
      <img style="margin: 0 auto; width:150px; text-align: center;" src="https://www.nicepng.com/png/detail/73-730285_it-benefits-per-users-icon-user-png-green.png" class="card-img-top"
        alt="...">
      <div class="card-body">
        <h1 style="text-align: center; font-size:13px;" class="card-text"> Full Name: {{this.first_name}} {{this.last_name}}</h1>
        <h1 style="text-align: center; font-size:13px;" class="card-text"> Email: {{this.email}}</h1>
        <h2 style="text-align: center; font-size:13px;" class="card-text"> Age: {{this.age}}</h2>
        <h2 style="text-align: center; font-size:13px;" class="card-text"> Role: {{this.role}}</h2>

        <button onclick="deleteUser('{{this._id}}')" style="display: block; margin: 0 auto;"
          class="btn btn-danger"> DELETE </button>

      </div>
    </div>
    {{/each}}
  </div>
</div>

